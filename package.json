{
    "name": "backend",
    "version": "0.1.3",
    "description": "Backend API for careerendorsement.com",
    "main": "src",
    "scripts": {
        "dev": "nodemon -r dotenv/config .",
        "format": "prettier --write \"**/*.{js,scss,css,html,json,md,less,tsx}\"",
        "knex:migrate:make": "knex migrate:make",
        "knex:migrate:latest": "knex migrate:latest",
        "knex:seed:make": "knex seed:make",
        "knex:rollback:all": "knex migrate:rollback --all",
        "knex:seed:run": "knex seed:run",
        "start": "node -r dotenv/config .",
        "test": "(dropdb test_career_endorsement && createdb test_career_endorsement) && jest --runInBand"
    },
    "repository": "git@github.com:labs15-career-endorsement-tracker/backend.git",
    "author": "Labs15SuperTeam",
    "license": "MIT",
    "private": true,
    "devDependencies": {
        "faker": "^4.1.0",
        "jest": "^24.9.0",
        "knex-cleaner": "^1.3.0",
        "nodemon": "^1.19.1",
        "prettier": "^1.18.2",
        "supertest": "^4.0.2"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "helmet": "^3.20.0",
        "http-errors": "^1.7.3",
        "knex": "^0.19.1",
        "morgan": "^1.9.1",
        "pg": "^7.12.1"
    }
}
